<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping date-format="yyyy-MM-dd">
        <class-a>com.decisioncustom.domain.TArticle</class-a>
        <class-b>com.decisioncustom.domain.vo.ArticleVO</class-b>
        <field>
            <a>createTimeStr</a>
            <b>createTimeStr</b>
        </field>
    </mapping>
    <mapping date-format="yyyy-MM-dd HH:mm:ss">
        <class-a>com.decisioncustom.domain.TVideo</class-a>
        <class-b>com.decisioncustom.domain.vo.VideoVO</class-b>
        <field>
            <a>creationTime</a>
            <b>createTimeStr</b>
        </field>
    </mapping>
    <mapping date-format="yyyy-MM-dd HH:mm:ss">
        <class-a>com.decisioncustom.domain.TVideo</class-a>
        <class-b>com.decisioncustom.domain.vo.VideoInfoVO</class-b>
        <field>
            <a>creationTime</a>
            <b>createTimeStr</b>
        </field>
    </mapping>
    <mapping date-format="yyyy-MM-dd HH:mm:ss">
        <class-a>com.decisioncustom.domain.TEmployee</class-a>
        <class-b>com.decisioncustom.domain.vo.EmployeeVO</class-b>
        <field>
            <a>realName</a>
            <b>realName</b>
        </field>
    </mapping>
</mappings>